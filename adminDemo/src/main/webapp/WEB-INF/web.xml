<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
         
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.admin.Loginservlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/log</url-pattern>
	</servlet-mapping>
	<servlet>
	<servlet-name>Registration</servlet-name>
	<servlet-class>com.customer.Customerinsert</servlet-class>
	</servlet>
	<servlet-mapping>
	<servlet-name>Registration</servlet-name>
	<url-pattern>/insert2</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
    	<servlet-name>newPlannerInsert</servlet-name>
    	<servlet-class>com.admin.PlannerInsertServlet</servlet-class>
    </servlet>
    <servlet-mapping>
    	<servlet-name>newPlannerInsert</servlet-name>
    	<url-pattern>/insert/*</url-pattern>
    </servlet-mapping>
    
    
    <servlet>
    	<servlet-name>searchPlanner</servlet-name>
    	<servlet-class>com.admin.SearchEmpServlet</servlet-class>
    </servlet>
    <servlet-mapping>
    	<servlet-name>searchPlanner</servlet-name>
    	<url-pattern>/search/*</url-pattern>
    </servlet-mapping>
    
     <servlet>
    	<servlet-name>UpdatePlanner</servlet-name>
    	<servlet-class>com.admin.PlannerUpdateServlet</servlet-class>
    </servlet>
    <servlet-mapping>
    	<servlet-name>UpdatePlanner</servlet-name>
    	<url-pattern>/update</url-pattern>
    </servlet-mapping>
    
    <servlet>
    	<servlet-name>DeletePlanner</servlet-name>
    	<servlet-class>com.admin.PlannerDeleletServlet</servlet-class>
    </servlet>
    <servlet-mapping>
    	<servlet-name>DeletePlanner</servlet-name>
    	<url-pattern>/delete</url-pattern>
    </servlet-mapping>
   
	
</web-app>
	
    